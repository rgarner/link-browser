{
  "manifest_version": 2,

  "name": "Link tag browser",
  "description": "Shows <link> tags as a browseable dropdown",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["link-reader.js"]
    }
  ]
}